    hito:operatingSystem dbr:Linux, dbr:Mac_OS, dbr:Microsoft_Windows, dbr:Unix ;
    hito:programmingLanguage <http://dbpedia.org/resource/C++> ;
    hito:usesStandard hito:Dicom, hito:Rest ;
    a hito:SoftwareProduct ;
    rdfs:comment "The enterprise function citations are based on the medfloss website." ;
    rdfs:label "Orthanc" .

