#### Other ##################################################################

:SoftwareProductInstallationShape a sh:NodeShape;
	sh:targetClass :SoftwareProductInstallation;
	sh:property :labelShape,
	[sh:path :softwareProductInstallationComponent],
	[sh:path :isOfType],
	[sh:path :installationSupportsEnterpriseFunction],
	[
		sh:path [ sh:inversePath :softwareProductInstallation ];
		sh:minCount 1;
		sh:maxCount 1
	];
	sh:closed true;
	sh:ignoredProperties :ignoredProperties .

##### Evaluates Domain ######################################################

:EvaluatesDomainShape a sh:NodeShape;
	sh:targetSubjectsOf
		:evaluatesApplicationSystem,
		:evaluatesApplicationSystemBasedOnProduct,
		:evaluatesApplicationSystemHavingFeature;
	sh:class :Study.

##### Evaluates Range #######################################################

:EvaluatesApplicationSystemRangeShape a sh:NodeShape;
	sh:targetObjectsOf :evaluatesApplicationSystem;
	sh:class :ApplicationSystemCitation.

