:ignoredProperties rdf:first vann:example;
	rdf:rest ( rdf:type rdfs:comment rdfs:seeAlso skos:altLabel dcterms:source :internalId skos:definition dce:source owl:sameAs rdfs:isDefinedBy).

:labelShape a sh:PropertyShape;
	sh:path rdfs:label;
	sh:minCount 1.

:commentShape a sh:PropertyShape;
	sh:path rdfs:comment;
	sh:minCount 1.

#### Software Product #######################################################

:SoftwareProductShape a sh:NodeShape;
	sh:targetClass :SoftwareProduct;
	sh:property :labelShape,
	[sh:path :interoperability],
	[sh:path :certification],
	[sh:path :operatingSystem],
	[sh:path :client],
	[sh:path :language],
	[sh:path :programmingLanguage],
	[sh:path :databaseSystem],
	[sh:path :license],
	[sh:path :programmingLibrary],
	[sh:path :homepage],
	[sh:path :fOffersFCit],
	[sh:path :efSupportsEfCit],
	[sh:path :astOffersAstCit],
	[sh:path :ouOffersOuCit],
	[sh:path :userOffersUserCit],
	[sh:path :fOffersFCla],
	[sh:path :efOffersEfCla],
	[sh:path :astOffersAstCla],
	[sh:path :ouOffersOuCla],
	[sh:path :userOffersUserCla],
	[sh:path :repository],
	[sh:path :softwareProductComponent];
	sh:closed true;
	sh:ignoredProperties :ignoredProperties.

#### Software Product Domain ################################################

:SoftwareProductDomainShape a sh:NodeShape;
	sh:targetSubjectsOf
		:interoperability,
		:certification,
		:operatingSystem,
		:client,
		:language,
		:programmingLanguage,
		:databaseSystem,
		:license,
		:programmingLibrary,
		:homepage,
		:fOffersFCit,
		:efSupportsEfCit,
		:astOffersAstCit,
		:ouOffersOuCit,
		:userOffersUserCit,
		:fOffersFCla,
		:efOffersEfCla,
		:astOffersAstCla,
		:ouOffersOuCla,
		:userOffersUserCla,
		:repository,
		:softwareProductComponent;
	sh:class :SoftwareProduct.

